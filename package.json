{
  "name": "nuzlocke",
  "author": "Diego Ballesteros Castellanos",
  "description": "Nuzlocke tracker to record encounters with detailed information",
  "version": "5.4.4",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/diballesteros/nuzlocke/issues"
  },
  "nyc": {
    "exclude": [
      "src/serviceWorker.ts",
      "src/serviceWorkerRegistration.ts",
      "src/components/UpdateSW/UpdateSW.tsx",
      "src/components/Auth/Auth.tsx",
      "src/index.tsx"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/service-worker.ts",
      "<rootDir>/src/serviceWorkerRegistration.ts",
      "<rootDir>/src/components/UpdateSW/UpdateSW.tsx",
      "<rootDir>/src/index.tsx",
      "<rootDir>/src/components/Auth/Auth.tsx"
    ]
  },
  "private": true,
  "dependencies": {
    "@sentry/react": "^7.53.1",
    "@sentry/tracing": "^7.53.1",
    "@smogon/calc": "^0.7.0",
    "@supabase/supabase-js": "^1.35.7",
    "html-to-image": "^1.11.11",
    "i18next": "^22.5.0",
    "immer": "^10.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.12.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "react-window": "^1.8.9",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4",
    "typescript": "4.5.5",
    "zustand": "^4.3.8"
  },
  "scripts": {
    "analyze": "source-map-explorer ''build/static/js/*.js'",
    "start": "react-scripts start",
    "httpss": "HTTPS=true react-scripts start",
    "develop:ci": "CHOKIDAR_USEPOLLING=1 react-scripts -r @cypress/instrument-cra start --config-file=cypress.config.ts",
    "build": "react-scripts build",
    "e2e": "cypress open",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x} --ignore-pattern *.test.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.6",
    "@cypress/instrument-cra": "^1.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "cypress": "^12.13.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-localstorage-commands": "^2.2.3",
    "eslint": "8.4.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "source-map-explorer": "^2.5.3"
  }
}
